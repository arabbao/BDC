    <entry>
      <name>sync_data</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>DO $BODY$
   DECLARE 
   		v_where character varying;
		v_dst_pk_split character varying[];
   BEGIN
   	 if '${SYNC_MODE}' = 'full' then
		truncate table ${DST_SCHEMA_NAME}.${DST_TABLE_NAME};
	 	insert into ${DST_SCHEMA_NAME}.${DST_TABLE_NAME} select * from sync.${DST_TABLE_NAME};
		update manager.job_data_sync set udt = now() where job_id = ${JOB_ID};
	 elsif '${SYNC_MODE}' = 'merge' then
	  	select regexp_split_to_array('${DST_PK}', ',') into v_dst_pk_split;
		for i in array_lower(v_dst_pk_split, 1) .. array_upper(v_dst_pk_split, 1)
		loop
			if v_where is null then
				v_where := 'a.'||v_dst_pk_split[i]||' = b.'||v_dst_pk_split[i];
			else
				v_where := v_where||' and '||'a.'||v_dst_pk_split[i]||' = b.'||v_dst_pk_split[i];
			end if;
		end loop;
		execute 'delete from ${DST_SCHEMA_NAME}.${DST_TABLE_NAME} a using sync.${DST_TABLE_NAME} b where '||v_where;
	  	insert into ${DST_SCHEMA_NAME}.${DST_TABLE_NAME} select * from sync.${DST_TABLE_NAME};
	  	update manager.job_data_sync set incr_point = (select max(${DST_INCR_FIELD})::varchar from sync.${DST_TABLE_NAME}), udt = now() where job_id = ${JOB_ID};
	 else
	 	insert into ${DST_SCHEMA_NAME}.${DST_TABLE_NAME} select * from sync.${DST_TABLE_NAME};
	  	update manager.job_data_sync set incr_point = (select max(${DST_INCR_FIELD})::varchar from sync.${DST_TABLE_NAME}), udt = now() where job_id = ${JOB_ID};
	 end if;
   END;
$BODY$ LANGUAGE PLPGSQL;</sql>
